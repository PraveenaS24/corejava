package com.chainsys.intro;

public class Demoa {

	public static int DataA; //static global varaible
	public final int DataB;//immutable global variable (not static)
	public final static int DAtaC=300; // constant
	
	//constructor
	public Demoa(int dataB)
	{
		this.DataB=dataB;
		}
		public static void sayHello()
		{
			System.out.println("Hello Developer!!!");
		}
       public void greatUser()
       {
    	   System.out.println("Greetings USER!!!");
      }

   }
